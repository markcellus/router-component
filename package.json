{
  "name": "router-component",
  "version": "0.2.0",
  "description": "A simple router for web components",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkay581/router-component.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Mark Kennedy <mark@heyimmark.com> (heyimmark.com)",
  "engines": {
    "node": ">=8.9.0"
  },
  "keywords": [
    "router",
    "routing",
    "router element",
    "route management",
    "router component",
    "routing urls",
    "lit router",
    "polymer router"
  ],
  "license": "MIT",
  "main": "dist/router-component.js",
  "types": "src/router-component.ts",
  "devDependencies": {
    "@types/youtube": "0.0.34",
    "banner-cli": "0.11.1",
    "chai": "^4.1.2",
    "karma": "^3.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^6.0.1",
    "mocha": "^5.2.0",
    "rollup": "^0.65.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript": "^0.8.1",
    "sinon": "^6.3.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "test": "karma start karma.conf.js && tslint src/**/*",
    "test-server": "karma start karma.conf.js --single-run=false",
    "preversion": "npm test",
    "minify": "uglifyjs dist/router-component.js -o dist/router-component.min.js",
    "build-dist": "rm -rf dist/* && rollup -c && npm run minify",
    "version": "npm run build-dist && banner-cli dist/*.js && git add -A dist",
    "postversion": "git push && git push --tags",
    "start": "rollup -c --watch"
  }
}
