{
  "name": "router-component",
  "version": "0.16.2",
  "description": "A simple, declarative router for web components",
  "repository": {
    "type": "git",
    "url": "https://github.com/markcellus/router-component.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/markcellus/router-component",
  "author": "Mark Kennedy <mark@heyimmark.com> (heyimmark.com)",
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "lit router",
    "spa router",
    "webcomponent router",
    "custom element-router",
    "js router",
    "lit element router",
    "declarative router",
    "router component",
    "web-router",
    "router",
    "router element",
    "route management",
    "route component",
    "routing urls",
    "polymer router",
    "html router"
  ],
  "license": "MIT",
  "main": "dist/router-component.common.js",
  "module": "dist/router-component.js",
  "types": "dist/router-component.d.ts",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@open-wc/testing": "^3.0.3",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@web/dev-server-esbuild": "^0.2.12",
    "@web/dev-server-import-maps": "0.0.6",
    "@web/test-runner": "^0.13.5",
    "banner-cli": "^0.14.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.0.0",
    "husky": "^7.0.0",
    "lit-html": "^2.0.2",
    "mocha": "^9.0.2",
    "prettier": "^2.0.2",
    "pretty-quick": "^3.0.0",
    "rollup": "^2.3.3",
    "rollup-plugin-typescript2": "^0.31.1",
    "servor": "^4.0.2",
    "sinon": "^13.0.1",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "test": "npm run test:src && npm run lint",
    "test:src": "web-test-runner",
    "preversion": "npm test",
    "build": "rm -rf dist/* && rollup -c && npm run banner",
    "banner": "banner-cli dist/**/*.js",
    "start": "rollup -c --watch",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:format": "prettier --check '**/*'",
    "start-server": "servor ./ 3239 --browse"
  },
  "dependencies": {
    "query-selector-shadow-dom": "^1.0.0"
  }
}
